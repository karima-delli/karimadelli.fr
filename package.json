{
  "name": "karimadelli.fr",
  "description": "Karima Delli website",
  "engines": {
    "node": "13.5.x"
  },
  "scripts": {
    "install:plugin:gatsby-contentful-rich-text-reading-time": "cd plugins/gatsby-contentful-rich-text-reading-time/ && yarn && cd ../..",
    "install:plugin:gatsby-source-europarl": "cd plugins/gatsby-source-europarl/ && yarn && cd ../..",
    "postinstall": "yarn install:plugin:gatsby-contentful-rich-text-reading-time && yarn install:plugin:gatsby-source-europarl",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop --port 8002 --host 0.0.0.0",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint --ext js --ext jsx ."
  },
